{
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "args": [],
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "${workspaceRoot}/src/Archetype.Tests"
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
